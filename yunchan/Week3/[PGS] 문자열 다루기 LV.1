#include <string>
#include <cctype>  // isdigit 함수 사용을 위해
using namespace std;

bool solution(string s) {
    bool answer = true;
    int len = s.length();  // len 선언
    
    if (len==4 || len==6){
    // 문자열에서 숫자 찾기
        for (int i = 0; i < len; i++) {
            if (!isdigit(s[i])) {  // s[i]가 숫자인지 확인
                answer = false;
                break;  // 숫자를 찾으면 더 이상 탐색할 필요 없음
            }
        }
    }
    else{
        return answer=false;
    }
    return answer;
}
